{
  "name": "sa-template-2",
  "version": "2.3.5",
  "description": "sa-template-2",
  "keywords": [
    "template",
    "webpack",
    "pug",
    "sass",
    "postcss",
    "svg-sprite",
    "imagemin",
    "optimize",
    "compress"
  ],
  "main": "install.js",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:semakov_andrey/sa-template-2.git"
  },
  "author": "Semakov Andrey <semakov.andrey.web@yandex.ru>",
  "license": "ISC",
  "scripts": {
    "install": "node install.js",
    "module": "node module.js",
    "start": "node start.js",
    "build": "node build.js",
    "lint-css": "stylelint src/**/*.scss -f verbose --fix"
  },
  "config": {
    "directories": {
      "source": "src",
      "development": "tmp",
      "production": "build",
      "files": {
        "html": "",
