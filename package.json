        "js": "scripts",
        "fonts": "fonts",
        "images": "images",
        "sprite": "images",
        "other": ""
      }
    },
    "devServer": {
      "secure": true,
      "host": "0.0.0.0",
      "port": "3000"
    },
    "entries": {
      "main": "main"
    },
    "browsers": [
      "> 0.5%"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "del": "^3.0.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.11.0",
